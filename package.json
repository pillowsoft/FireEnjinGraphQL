{
  "name": "@enjin/fireenjingraphql",
  "engines": {
    "node": "10"
  },
  "version": "0.0.0-development",
  "scripts": {
    "build": "npm run clean && tsc",
    "build:firebase": "npm run build && node tasks/buildFirebaseFunctions.js",
    "clean": "rimraf dist/",
    "codegen": "graphql-codegen --config codegen.yml",
    "deploy:graphql": "npm run build && gcloud app deploy",
    "deploy:firebase": "firebase deploy",
    "env": "node tasks/env",
    "lint": "tslint --project tsconfig.json",
    "migrate": "npm run build && node tasks/migrate",
    "release": "npm run lint && npm run build && npm run codegen",
    "semantic-release": "semantic-release",
    "seed": "npm run build && node tasks/runSeeds",
    "seed:clone": "npm run build && node tasks/cloneSeed",
    "serve": "ts-node-dev --respawn src/index.ts",
    "start": "npm run env local && npm run serve",
    "test": "jest --no-cache",
    "test:watch": "jest --watch --no-cache"
  },
  "types": "dist/definitions.d.ts",
  "main": "dist/index.js",
  "dependencies": {
    "apollo-server": "^2.9.6",
    "firebase-admin": "^8.6.1",
    "firebase-functions": "^3.3.0",
    "fireorm": "^0.10.0",
    "fs-extra": "^8.1.0",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.1",
    "pluralize": "^8.0.0",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^0.17.5"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.8.1",
    "@graphql-codegen/fragment-matcher": "1.8.1",
    "@graphql-codegen/introspection": "1.8.1",
    "@graphql-codegen/typescript": "1.8.1",
    "@graphql-codegen/typescript-document-nodes": "1.8.1",
    "@graphql-codegen/typescript-graphql-files-modules": "1.8.1",
    "@graphql-codegen/typescript-operations": "1.8.1",
    "@graphql-codegen/typescript-stencil-apollo": "1.8.1",
    "@graphql-codegen/typescript-type-graphql": "^1.8.1",
    "@types/jest": "^24.0.19",
    "globby": "^10.0.1",
    "jest": "^24.9.0",
    "rimraf": "^3.0.0",
    "semantic-release": "^15.13.24",
    "ts-jest": "^24.1.0",
    "ts-node-dev": "^1.0.0-pre.43",
    "tslint": "^5.20.0",
    "typescript": "^3.6.4"
  },
  "author": {
    "email": "info@madnesslabs.net",
    "name": "Madness Labs",
    "url": "https://madnesslabs.net"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/madnesslabs/FireEnjinGraphQL.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(ts?)$",
    "testURL": "http://localhost",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
