{
  "name": "@enjin/fireenjin",
  "version": "0.0.0-development",
  "scripts": {
    "build": "tsc",
    "codegen": "graphql-codegen --config codegen.yml",
    "docs": "typedoc --mode modules",
    "deploy": "npm run build && gcloud app deploy",
    "semantic-release": "semantic-release",
    "serve": "ts-node-dev --respawn src/index.ts",
    "start": "npm run serve",
    "test": "jest --no-cache",
    "test:watch": "jest --watch --no-cache"
  },
  "main": "dist/index.js",
  "dependencies": {
    "apollo-server": "^2.9.4",
    "firebase-admin": "^8.6.0",
    "fireorm": "^0.9.0",
    "fs-extra": "^8.1.0",
    "glob": "^7.1.4",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.1",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^0.17.5"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.7.0",
    "@graphql-codegen/fragment-matcher": "1.7.0",
    "@graphql-codegen/introspection": "1.7.0",
    "@graphql-codegen/typescript": "1.7.0",
    "@graphql-codegen/typescript-document-nodes": "1.7.0",
    "@graphql-codegen/typescript-graphql-files-modules": "1.7.0",
    "@graphql-codegen/typescript-operations": "1.7.0",
    "@graphql-codegen/typescript-stencil-apollo": "1.7.0",
    "@graphql-codegen/typescript-type-graphql": "^1.7.0",
    "@types/jest": "^24.0.18",
    "jest": "^24.9.0",
    "semantic-release": "^15.13.24",
    "ts-jest": "^24.1.0",
    "ts-node-dev": "^1.0.0-pre.43",
    "tslint": "^5.20.0",
    "typescript": "^3.6.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/madnesslabs/FireEnjinGraphQL.git"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(ts?)$",
    "testURL": "http://localhost",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
