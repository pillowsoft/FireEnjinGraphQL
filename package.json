    "fireorm": "^0.7.0",
    "fs-extra": "^8.1.0",
    "graphql": "^14.5.4",
    "graphql-tag": "^2.10.1",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "concurrently": "^4.1.2",
    "nodemon": "^1.19.2",
    "semantic-release": "^15.13.24",
    "tslint": "^5.19.0",
    "typescript": "^3.6.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/madnesslabs/FireEnjinGraphQL.git"
